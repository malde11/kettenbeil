    display: grid;
    grid-gap: 25px;
    grid-template-columns: repeat(12, 1fr);
    grid-template-areas: "slug-wrapper slug-wrapper slug-wrapper slug-wrapper slug-wrapper slug-wrapper slug-wrapper slug-wrapper slug-wrapper slug-wrapper slug-wrapper slug-wrapper" "img-shadow img-shadow img-shadow img-shadow img-shadow img-shadow img-shadow img-shadow img-shadow img-shadow img-shadow img-shadow"; }
  .img-shadow {
    display: grid;
    grid-template-columns: repeat(12, 1fr);
    grid-gap: 25px;
    grid-template-areas: ". main-content main-content main-content main-content main-content main-content-2 main-content-2 main-content-2 main-content-2 main-content-2 ." ". main-content-3 main-content-3 main-content-3 main-content-3 main-content-3 main-content-4 main-content-4 main-content-4 main-content-4 main-content-4 ." ". main-content-6 main-content-6 main-content-6 main-content-6 main-content-6 main-content-5 main-content-5 main-content-5 main-content-5 main-content-5 ."; }
  .main-content,
  .main-content-4,
  .main-content-6 {
    margin-bottom: 0; }
  /* NAVBAR */
  .topnav a {
    font-size: 1.1em; }
  .topnav.responsive a {
    padding: 15px 0px; }
  .home-link {
    display: contents; }
  .main-headline {
    margin-top: 160px; }
  .logo-headline {
    margin-top: 60px; } }

/*******************************************************
  Media Queries - (desktops, 992px and up)
*******************************************************/
@media (min-width: 992px) {
