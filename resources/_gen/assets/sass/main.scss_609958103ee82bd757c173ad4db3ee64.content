.topnav{overflow:hidden;background-color:#333}.topnav a{text-align:right;display:none;color:#f2f2f2;text-align:center;padding:14px 16px;text-decoration:none;font-size:24px}.topnav a:hover{background-color:#ddd;color:#000}.topnav a.active{background-color:#04aa6d;color:#fff;display:block;float:left}.topnav .icon{display:block}@media(min-width:768px){.topnav a:not(:first-child){display:block}.topnav a.icon{float:right;display:none}}@media(min-width:768px){.topnav.responsive{position:relative}.topnav.responsive .icon{position:absolute;right:0;top:0}.topnav.responsive a{float:none;display:block;text-align:left}}