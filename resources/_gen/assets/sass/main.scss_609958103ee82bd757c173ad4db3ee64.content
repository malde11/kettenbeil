*{margin:0;padding:0}:root{--data-color-primary:#00a2ff;--data-color-secondary:#333}@font-face{font-family:smooch sans;font-optical-sizing:auto;font-weight:500;font-style:normal;src:url(/fonts/SmoochSans-Medium.ttf)}.container{display:grid;height:100vh;grid-template-columns:1fr;grid-template-rows:.1fr 1fr 1fr 1fr;grid-template-areas:"nav" "header" "main" "footer"}nav{grid-area:nav;background-color:#000}header{grid-area:header;background-color:#000;z-index:-3}main{grid-area:main;background-color:#d2691e}footer{grid-area:footer;background-color:#6495ed}#loader-background{position:fixed;top:0;left:0;width:100%;height:100%;background-color:#000;z-index:1021}#loader{position:fixed;top:25%;left:25%;width:160px;height:160px;background-image:url(/images/preloader/loader_small.svg);background-repeat:no-repeat;z-index:1022;-webkit-animation:fadein 2s infinite;animation:fadein 2s infinite}nav{position:fixed;top:0;left:0;right:0;width:100vw;background-color:#2c2c2c;z-index:1}#burgerMenu{float:right;padding:28px 15px 15px}.dot-arrow{padding-right:10px}.topnav a{display:none;color:#fff;text-decoration:none;font-size:20px}.topnav.responsive .icon{position:absolute;right:0;top:0;z-index:2}.topnav.responsive a{display:block;float:none;padding:16px 0}.responsive{margin-top:28px;padding:0 0 10px 20px;background-color:#161616}.toggleMenu{content:url(/images/svg/menu-close.svg);width:32px;height:32px}.nav-item{display:inline;padding:0 10px 10px;font-size:1.5rem;color:#fff;text-transform:uppercase;text-shadow:10px 0 20px #00a2ff,10px 0 20px #00a2ff}.nav-link{text-decoration:none}.dot-arrow{padding-right:10px}.navbar-brand{font-family:smooch sans,serif;color:#fff;text-decoration:none;font-size:2.2rem;text-shadow:10px 0 20px #00a2ff,10px 0 20px #00a2ff}.logo-brand-wrapper{display:inline}.brand-logo{padding:25px 5px 0 15px;text-shadow:1px 1px 2px #000,0 0 25px #00a2ff,10px 0 20px #004f7c}.brand-name{color:#00a2ff}.nav-link:link{color:#fff;text-decoration:none}.nav-link:visited{color:#fff}.nav-link:hover{color:#fff}.nav-link:active{color:#fff}.nav-link:hover::before{content:"[";display:none;color:#00a2ff}.nav-link:hover::after{content:"]";display:none;color:#00a2ff}#video-wrapper{background:url(/images/backgrounds/background.webp);background-size:cover;background-position:50%;background-repeat:no-repeat;height:100%;width:100%;-o-object-fit:cover;object-fit:cover;top:0;bottom:0;position:absolute;z-index:-2}video{background-size:cover;background-position:50%;background-repeat:no-repeat;width:100%;height:100%;object-fit:cover;opacity:.6;top:0;bottom:0;position:absolute;z-index:-1}.video-section{position:relative;height:100vh;background-size:cover;background-position:50%;display:flex;align-items:center;justify-content:center}@media(min-width:576px){.nav-item{font-size:.8rem}.topnav.responsive a{padding:5px 0}}@media(min-width:768px){.topnav.responsive a{padding:5px 0}}@media(min-width:992px){.container{display:grid;height:100vh;grid-template-columns:1fr 1fr 1fr;grid-template-rows:1fr 1fr 1fr;grid-template-areas:"nav nav nav" "header header header" "main main main" "footer footer footer"}.topnav{float:right;padding:20px}.topnav a{display:inline;font-size:1.3rem}#burgerMenu{display:none}.brand-logo{padding:13px 5px 0 15px;width:30px;height:30px}.navbar-brand{font-size:2.3rem}.dot-arrow{display:none}.nav-item{position:relative}.nav-link:hover::before{content:"[";display:inline;position:absolute;left:-2px}.nav-link:hover::after{content:"]";display:inline;position:absolute;right:2px}}@media(min-width:1400px){#loader{top:35%;left:40%;width:360px;height:360px;background-image:url(/images/loader.svg)}}